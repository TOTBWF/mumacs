# key: !tikztri
# name: commutative-triangle
# expand-env: ((yas-indent-line 'fixed) (yas-wrap-around-region 'nil))
# --
\\[
\\begin{tikzcd}
  {$1} & & {$2} \\\\
  & {$3} & \\\\
  \\arrow["{$4}", from=1-1, to=1-3]
  \\arrow["{$5}"', from=1-1, to=2-2]
  \\arrow["{$6}", from=1-3, to=2-2]
\\end{tikzcd}
\\]
